{
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"extends": ["airbnb-base", "prettier"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"no-plusplus": 0,
		"no-param-reassign": 0
	},
	"plugins": ["html", "import", "prettier"]
}
